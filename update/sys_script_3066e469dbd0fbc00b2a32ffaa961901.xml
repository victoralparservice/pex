        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	var type = current.type.toString();
	var survey = current.sys_id.toString();

	var matrixUsers = [];

	var gr = new GlideRecord("x_alabs_pex_location_survey_types");
	gr.addQuery("survey_type", type);
	gr.query();

	while (gr.next()) {
		matrixUsers.push(gr.responsible.toString());
	}

	matrixUsers.forEach(function(user) {
		gr = new GlideRecord("x_alabs_pex_instance_survey");
		gr.initialize();
		gr.responsible = user;
		gr.survey = survey;
		gr.insert();
	});
})(current, previous);
]]></script>
